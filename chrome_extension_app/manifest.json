{
    "manifest_version": 3,
    "name": "إضافة متصفح بسيطة",
    "version": "1.0",
    "description": "إضافة كروم تجريبية تم إنشاؤها كجزء من دورة JavaScript.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ]
}
